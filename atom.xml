<p><img src="../images/13-12-12/a.jpg" alt="" /></p>

<pre><code>void genRandom(int m,int n){
 int select = m;
 ofstream writer("data.txt");
 for(int i=0;i&lt;n;++i){
   if(rand() % (n-i) &lt; select){
     writer &lt;&lt; i &lt;&lt; endl;
     -- select;
   }
 }
 writer.close();
}
